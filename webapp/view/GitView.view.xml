<mvc:View controllerName="first.myGitDemo.controller.GitView" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<List items="{/fruits}">
							<items>
								<ObjectListItem title="{fruitName} Color:{color}" number="{price}" numberUnit="{priceQty}" icon="{image}" intro="{healthBenfit}"></ObjectListItem>
							</items>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>